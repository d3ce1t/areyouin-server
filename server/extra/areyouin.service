[Unit]
Description=AreYouIN Service
After=network.target cassandra.service
Wants=cassandra.service

[Service]
Type=simple
ExecStart=/opt/areyouin/areyouind
WorkingDirectory=/opt/areyouin
User=areyouin
PrivateTmp=yes
InaccessibleDirectories=/home
#ReadOnlyDirectories=/var
Restart=on-failure

[Install]
WantedBy=multi-user.target